<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="getMemoryFree">
    <Locals>
      <Local name="l_nSpace" type="num" xsi:type="array" size="1" />
    </Locals>
    <Code><![CDATA[begin
  //
  l_nSpace=$getFreeSpace("/usr")
  gotoxy(1,9)
  put("sys free space:"+toString(".2",l_nSpace))
end]]></Code>
  </Program>
</Programs>